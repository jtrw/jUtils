{
    "name": "jtrw/utils",
    "type": "library",
    "description": "Utils",
    "keywords": [
        "nilBora",
        "Utils",
        "ValuesObject"
    ],
    "homepage": "https://github.com/jtrw/utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Nil",
            "email": "brdnlsrg@gmail.com",
            "homepage": "https://github.com/jtrw",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jtrw\\Utils\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jtrw\\Utils\\Tests\\": "tests",
            "Jtrw\\Utils\\Tests\\Resources\\": "tests/Resources"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
